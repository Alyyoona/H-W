#Написать класс связанных списков
#Написать класс очереди и стека, используя внутри только списки.
class Stack:

    def __init__(self):
        self.stack_list = []

    def __len__(self):
        return len(self.stack_list)

    def re(self):
        return self.stack_list.pop()

    def up(self, num: int):
        self.stack_list.append(num)

    def top(self):
        return self.stack_list[-1]


class Queue:

    def __init__(self):
        self.queue_list = []

    def __len__(self):
        return len(self.queue_list)

    def de(self):
        return self.queue_list.pop()

    def turn(self, num: int):
        self.queue_list.insert(0, num)

    def last(self):
        return self.queue_list[0]

    def first(self):
        return self.queue_list[-1]
